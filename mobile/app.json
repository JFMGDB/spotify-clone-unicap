{
  "expo": {
    "name": "Spotify Clone",
    "slug": "spotify-clone-unicap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "spotify-clone",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.unicap.spotifyclone"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#191414",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "package": "com.unicap.spotifyclone",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#191414",
          "dark": {
            "backgroundColor": "#191414"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "apiUrl": "https://spotify-clone-unicap.vercel.app",
      "router": {},
      "eas": {
        "projectId": "46852c7d-1f40-4ade-9ae0-31d27e4e0530"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/46852c7d-1f40-4ade-9ae0-31d27e4e0530"
    },
    "owner": "andersonmsmarinho"
  }
}
